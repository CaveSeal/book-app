{
  "name": "book-app",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "server": "nodemon src/server/app.js",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier --fix"
    ]
  },
  "nodemonConfig": {
    "watch": "src/server/",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,jsx,json",
    "timeout": 2000,
    "verbose": false
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.10.0",
    "connect-mongo": "^3.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http-errors": "^1.7.3",
    "mongoose": "^5.9.18",
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "license": "MIT"
}
